{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "yunhanphp/lumen-require": "dev-master",
        "doctrine/dbal": "^2.8",
        "jmd-backend/php-libs": "dev-master",
        "illuminate/mail": "5.6.*",
        "aliyuncs/oss-sdk-php": "~2.0",
        "spatie/laravel-permission": "^2.12",
        "zhuzhichao/ip-location-zh": "~2.4",
        "jenssegers/agent": "^2.6",
        "tymon/jwt-auth": "1.0.0-rc.2",
        "mpdf/mpdf": "^7.1",
        "ramsey/uuid": "^3.8",
        "jacobcyl/ali-oss-storage": "^2.1",
        "league/flysystem": "^1.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "maatwebsite/excel": "^3.0",
        "lingan/dingtalk-notice": "^0.1.3",
        "pusher/pusher-php-server": "^4.1",
        "razorpay/razorpay": "2.*",
        "brozot/laravel-fcm": "^1.3",
        "torann/geoip": "1.1.1",
        "cmdotcom/text-sdk-php": "^2.0",
        "milon/barcode": "6.0.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "yunhanphp/lumen-require-dev": "dev-master",
        "phan/phan": "^1.2",
        "spatie/laravel-backup": "^5.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Common\\": "apps/_common",
            "Main\\": "apps/main",
            "Admin\\": "apps/admin",
            "Api\\": "apps/api",
            "Console\\": "apps/console",
            "Test\\": "apps/test",
            "Tests\\": "tests/",
            "CashNow\\": "apps/cashnow",
            "Approve\\Admin\\": "apps/approve/admin",
            "Risk\\": "apps/risk"
        },
        "files": [
            "apps/_common/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-install-cmd": [
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "repositories": {
        "lumen-require-dev": {
            "type": "git",
            "url": "https://github.com/YunhanPHP/lumen-require-dev.git"
        },
        "lumen-dev-db-doc": {
            "type": "git",
            "url": "https://github.com/YunhanPHP/lumen-dev-db-doc.git"
        },
        "lumen-dev-yaml-swagger": {
            "type": "git",
            "url": "https://github.com/YunhanPHP/lumen-dev-yaml-swagger.git"
        },
        "lumen-require": {
            "type": "git",
            "url": "https://github.com/windysay/lumen-require.git"
        },
        "php-libs": {
            "type": "git",
            "url": "https://github.com/niowang/php-libs.git"
        }
    }
}
